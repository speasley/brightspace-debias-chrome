{
    "manifest_version": 3,
    "name": "Brightspace Debias",
    "version": "0.1.3",
    "description": "Obfuscate learner details from the Brightspace UI to reduce evaluation bias.",
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "content_scripts": [
        {
            "js": ["scripts/content.js"],
            "matches": ["http://*/*", "https://*/*"]
        }
    ],
    "permissions": [
        "activeTab"
    ],
    "author": "Stephen Peasley",  
    "homepage_url": "https://github.com/speasley/brightspace-debias-chrome"
  }